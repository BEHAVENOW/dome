/* 表格开始 */
.table {
  border: 0px solid rgb(0, 0, 0);
  margin-top: 20px;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.list {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.td {
  flex: 1;
  justify-content: center;
  background: #ffffff;
  font-size: 20rpx;
  color: rgb(0, 0, 0);
  display: flex;
  height: 2rem;
  align-items: center;
  border: 1px solid rgb(0, 0, 0);
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  flex: 1;
  justify-content: center;
  background: #ffffff;
  font-size: 20rpx;
  color: rgb(0, 0, 0);
  display: flex;
  height: 2rem;
  align-items: center;
  border: 1px solid rgb(0, 0, 0);
}

.th-th1 {
  border-left: 0;
}
/* 表格结束 */