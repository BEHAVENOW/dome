.body {
  width: 100%;
  height: 100vh;
  color: rgb(255, 255, 255);
  background: #303030;
}

/* 表格开始 */
.table {
  border: 0px solid rgb(255, 255, 255);
  margin-top: 20px;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.list {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}
.td {
  flex: 1;
  justify-content: center;
  background: #303030;
  font-size: 20rpx;
  color: rgb(255, 255, 255);
  display: flex;
  height: 2rem;
  align-items: center;
  border: 1px solid rgb(255, 255, 255);
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #303030;
}
.th {
  flex: 1;
  justify-content: center;
  background: #303030;
  font-size: 20rpx;
  color: rgb(255, 255, 255);
  display: flex;
  height: 2rem;
  align-items: center;
  border: 1px solid rgb(255, 255, 255);
}

.th-th1 {
  border-left: 0;
}
/* 表格结束 */